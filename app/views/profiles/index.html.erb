<p id="notice"><%= notice %></p>

<h1>Listing Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Class age</th>
      <th>Class gender</th>
      <th>Class language</th>
      <th>Class country</th>
      <th>Class biography</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.class_age %></td>
        <td><%= profile.class_gender %></td>
        <td><%= profile.class_language %></td>
        <td><%= profile.class_location %></td>
        <td><%= profile.class_biography %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
